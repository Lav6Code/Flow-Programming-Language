import turtle

# Set up the screen
screen = turtle.Screen()

# Create a turtle
t = turtle.Turtle()

# Rotate the turtle to 45 degrees
t.setheading(45)

# Write text at the rotated angle
t.write("Hello, World!", font=("Arial", 16, "normal"))

# Hide the turtle
t.hideturtle()

# Keep the window open
turtle.done()
